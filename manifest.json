{
    "manifest_version": 3,
    "name": "LinkedIn Auto Message Personalizer",
    "version": "1.0",
    "description": "Generate smart messages based on Linked In profiles",
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
    },
    "permissions": ["activeTab", "scripting"],
    "icons": {
      "48": "icon.png"
    }
  }

  